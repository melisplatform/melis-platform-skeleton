{
    "name": "melisplatform/melis-platform-skeleton",
    "description": "Melis Platform Skeleton",
    "type": "project",
    "license": "OSL-3.0",
    "keywords": [
        "melis",
        "melisplatform",
        "platform",
        "laminas",
        "mvc",
        "framework"
    ],
    "require": {
        "php": "^7.1",
        "ext-openssl": "*",
        "ext-json": "*",
        "ext-pdo_mysql": "*",
        "ext-intl": "*",
        "composer/composer": "1.9.3",
        "laminas/laminas-mvc": "^3.1.1",
        "laminas/laminas-i18n": "^2.10",
        "laminas/laminas-session": "^2.9",
        "laminas/laminas-form": "^2.14",
        "laminas/laminas-hydrator": "^2.4",
        "laminas/laminas-mvc-i18n": "^1.1",
        "laminas/laminas-db": "^2.11",
        "laminas/laminas-authentication": "^2.7",
        "laminas/laminas-mime": "^2.7",
        "laminas/laminas-inputfilter": "^2.10",
        "laminas/laminas-file": "^2.8",
        "laminas/laminas-development-mode": "^3.2",
        "melisplatform/melis-asset-manager": "dev-migrate/laminas as 3.2",
        "melisplatform/melis-composerdeploy": "dev-migrate/laminas as 3.2",
        "melisplatform/melis-core": "dev-migrate/laminas as 3.2",
        "melisplatform/melis-dbdeploy": "dev-migrate/laminas as 3.2",
        "melisplatform/melis-installer": "dev-migrate/laminas as 3.2",
        "melisplatform/melis-marketplace": "dev-migrate/laminas as 3.2"
    },
    "scripts": {
        "post-update-cmd": [
          "MelisCore\\ModuleComposerScript::executeScripts",
          "MelisDbDeploy\\DbDeployOnComposerUpdate::postUpdate"
        ],
        "development-disable": "laminas-development-mode disable",
        "development-enable": "laminas-development-mode enable",
        "development-status": "laminas-development-mode status",
        "post-create-project-cmd": "php bin/update-gitignore.php"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "laminas/laminas-code": "^3.4",
        "laminas/laminas-developer-tools": "^2.0"
    }
}
