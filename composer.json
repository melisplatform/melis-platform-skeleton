{
	"name": "melisplatform/melis-platform-skeleton",
	"description": "Melis Platform Skeleton",
	"type": "project",
	"license": "OSL-3.0",
	"keywords": [
		"melis",
		"melisplatform",
		"platform",
		"zf2",
		"laminas",
		"mvc",
		"framework"
	],
	"require": {
		"php": "^8.1|^8.3",
		"melisplatform/melis-asset-manager": "dev-update/php83 as 5.0",
		"melisplatform/melis-composerdeploy": "dev-update/php83 as 5.0",
		"melisplatform/melis-core": "dev-update/php83 as 5.0",
		"melisplatform/melis-dbdeploy": "dev-update/php83 as 5.0",
		"melisplatform/melis-installer": "dev-update/php83",
		"melisplatform/melis-marketplace": "*"
	},
	"scripts": {
		"post-update-cmd": [
			"MelisCore\\ModuleComposerScript::executeScripts",
			"MelisDbDeploy\\DbDeployOnComposerUpdate::postUpdate"
		],
		"development-disable": "laminas-development-mode disable",
		"development-enable": "laminas-development-mode enable",
		"development-status": "laminas-development-mode status"
	},
	"config": {
		"platform-check": false,
		"allow-plugins": {
			"symfony/flex": false,
			"symfony/runtime": true
		}
	}
}
